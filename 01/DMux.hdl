/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here:
    Not(in=a,out=n1a);
    Not(in=b,out=n2b);
    Nand(a=n1a,b=n2b,out=out);
}
